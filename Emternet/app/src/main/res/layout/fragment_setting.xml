<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/setting_background">


    <ImageButton
        android:id="@+id/button"
        android:layout_width="289dp"
        android:layout_height="85dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/ic_setting_1"
        android:text="使用說明"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/button2"
        android:layout_width="289dp"
        android:layout_height="85dp"
        android:layout_marginTop="24dp"
        android:text="常見問題"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button"
        android:background="@drawable/ic_setting_2"/>

    <ImageButton
        android:id="@+id/button3"
        android:layout_width="289dp"
        android:layout_height="85dp"
        android:layout_marginTop="24dp"
        android:text="聯絡我們"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        android:background="@drawable/ic_setting_3"/>

    <ImageButton
        android:id="@+id/button4"
        android:layout_width="289dp"
        android:layout_height="85dp"
        android:layout_marginTop="24dp"
        android:text="開發者人員"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        android:background="@drawable/ic_setting_4"/>

    <ImageButton
        android:id="@+id/button5"
        android:layout_width="289dp"
        android:layout_height="85dp"
        android:layout_marginTop="24dp"
        android:text="登出"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        android:background="@drawable/ic_setting_5"/>

</androidx.constraintlayout.widget.ConstraintLayout>